{
  "name": "udc-webstack2.0",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server.cjs",
    "server:dev": "cross-env NODE_ENV=development node server.cjs",
    "server:prod": "cross-env NODE_ENV=production node server.cjs",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "dev:custom": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "start": "npm run build && npm run server:prod",
    "deploy": "./deploy.sh",
    "deploy:netlify": "netlify deploy --prod"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@sendgrid/mail": "^8.1.6",
    "@types/formidable": "^3.4.5",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "formidable": "^3.5.4",
    "nodemailer": "^7.0.7",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.64.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.3",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript-eslint": "^8.45.0"
  }
}
