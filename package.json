{
  "name": "udc-webstack2.0",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:optimized": "npm run optimize:images && tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "start": "npm run build && npm run preview",
    "deploy": "npm run build:optimized && npm run worker:deploy",
    "deploy:frontend": "npm run build:optimized",
    "deploy:backend": "npm run worker:deploy",
    "deploy:full": "npm run deploy:backend && npm run deploy:frontend",
    "worker:dev": "cd workers && wrangler dev",
    "worker:deploy": "cd workers && wrangler deploy",
    "worker:tail": "cd workers && wrangler tail",
    "worker:test": "cd workers && node test-worker.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run worker:dev\"",
    "install:worker": "cd workers && npm install",
    "optimize:images": "node optimize-images.js",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.64.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.3",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "sharp": "^0.34.4",
    "terser": "^5.44.0",
    "typescript-eslint": "^8.45.0"
  }
}
